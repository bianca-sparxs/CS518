formulas(assumptions).

  % five axioms of group theory, not all of them are necessary 

  m(m(x,y),z) = m(x,m(y,z)).    % m is associative
  m(x,e) = x.                   % e is a right identity
  m(x,i(x)) = e.                % i(x) is right inverse of x

end_of_list.

formulas(goals).
  i(m(a, b)) = m(i(a), i(b)).
end_of_list.